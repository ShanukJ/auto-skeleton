{
  "name": "auto-skeleton-react",
  "version": "1.0.0",
  "description": "Auto-generate skeleton loading screens from your existing React DOM structure",
  "type": "module",
  "main": "dist/auto-skeleton-react.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/auto-skeleton-react.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc -p tsconfig.lib.json",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "react",
    "skeleton",
    "loading",
    "auto-skeleton",
    "placeholder",
    "skeleton-screen",
    "loading-state"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  }
}
